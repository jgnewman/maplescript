(elem Title [props children]
  <h1 {
    :class props:class
    :data-bar props:data-bar
    :aria-label props:aria-label
    :tabIndex props:tab-index
    :onclick (fn [] (log 'worked!'))}>
    <span>children</span>
  </h1>
)

(make title1
  <Title {
    :class 'foo'
    :data-bar 'bar'
    :aria-label 'baz'
    :tabindex 1
  }> 'Hello world!' </Title>
)

(make title2
  <Title {
    :class 'foo2'
    :data-bar 'bar2'
    :aria-label 'baz2'
    :tabindex 2
  }> 'Goodbye world!' </Title>
)

(make renderedTitle (vdom:render title1))

(vdom:injectNodes renderedTitle '#app')

(setTimeout (fn []
  (make patches (vdom:diff title1 title2))
  (vdom:patchNodes renderedTitle patches)
) 5000)
